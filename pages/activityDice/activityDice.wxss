/* 摇骰子区域 */
.dice {
  background: url(https://image.prise.shop/images/2018/09/04/1536054958395174.png) no-repeat;
  width: 360rpx;
  height: 360rpx;
  background-size: contain;
  margin: 20rpx auto 0;
  text-align: center;
  padding-top: 40rpx;
}
.dice .default-dice {
  width: 280rpx;
  height: 280rpx;
}
.dice .active-dice {
  animation: dice-rotate 0.3s 2 linear;
}
.default {
  animation: dice-shake 4s infinite linear;
}
.gif-dice {
  width: 200rpx;
  height: 200rpx;
}
/* 骰子动画 */
@keyframes dice-shake {
     0% {
       transform:rotate(0);
      }
     15%{
       transform:rotate(-5deg) ;
       }
     30%{
       transform:rotate(0) ;
      }
      65%{
        transform:rotate(20deg) ;
      }
      100%{
        transform:rotate(0) ;
      }
}
@keyframes dice-rotate {
     0% {
       transform:rotate(0);
      }
      
      100%{
        transform:rotate(360deg) ;
      }
}

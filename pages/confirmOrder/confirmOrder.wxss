.modal {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 10;
}
.mask {
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.4);
  top: 0;
  left: 0
}
.modal-container {
  width: 540rpx;
  height: 312rpx;
  background: #fff;
  border-radius: 20rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  font-size: 34rpx;
  text-align: center;
}
.modal-title {
  color: #030303;
  margin: 60rpx auto 30rpx;
}
.modal-content{
  color: #999999;
  font-size: 26rpx;  
}
.modal-footer {
  height: 88rpx;
  width: 100%;
  position: absolute;
  bottom: 0;
  border-top: 1rpx solid #eee;
}
.modal-footer view {
  width: 50%;
  display: inline-block;
  line-height: 88rpx;
  height: 88rpx;
  box-sizing: border-box;
}
.leave {
  color: #999999;
  font-size: 34rpx;
  border-right: 1rpx solid #eee;
}

.add-address {
  height: 108rpx;
  background: #fff;
  width: 100%;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
  /* border-top: 1rpx solid #d8d8d8; */
  position: relative;
  /* flex-wrap: wrap */
}
.add-address::after,.address-info::after{
  position: absolute;
  top: 0;
  width: 100%;
  left: 0;
  content: '';
  overflow: hidden;
  height: 1rpx;
  background: #d8d8d8;
  transform: scaleY(0.5)
}
.add-address text {
  flex: 2
}
.position {
  width: 38rpx;
  margin-right: 20rpx;
}
.location {
  margin-top: 10rpx; 
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.username {
  margin-right: 30rpx;
}
.address-info {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  padding: 0 30rpx;
  background: #fff;
  height: 176rpx;
  /* border-top: 1rpx solid #d8d8d8; */
  position: relative;
  width: 100%;
}
.address-info image {
  margin-left: 30rpx;
}
.address-left {
  align-self: flex-start;
  width: 650rpx;
  display: flex;
  flex-direction: column;
  height: 100%;
  justify-content: center;
}
.add-address .go,.address-info .go {
  width: 16rpx;
  /* height: 28rpx; */
}
/* 列表  */

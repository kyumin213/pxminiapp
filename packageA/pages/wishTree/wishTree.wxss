  line-height: 52rpx;
  margin: 0 6rpx 0 14rpx;
}
.water-scale {
  animation: waterScale 0.6s linear;
  animation-delay: 3.3s;
}
@keyframes waterScale {
  0% {
    transform: scale(1)
  }
  50% {
    transform: scale(1.5)
  }
  100% {
    transform: scale(1)
  }
}
.progress-bar {
  width: 354rpx;
  height: 26rpx;
  background: #079257;
  border-radius: 12rpx;
  position: relative;
}
.progress-value {
  /* width: 100rpx; */
  height: 26rpx;
  background: #1EE689;
  border-radius: 12rpx;
  transition: all .3s linear;
}
.progress-bar .numbers {
  color: #fff;
  font-size: 20rpx;
  position: absolute;
  left: 50%;
  top: 0;
  transform: translate(-50%,0);
}
.progress-bar text {font-size: 20rpx;}
.valtxt {min-width: 60rpx;text-align: right;display: inline-block;  }
/* value 动画 */
.animationValue {
  animation: scaleValue .8s linear;
  animation-delay: 1.2s;  
}
@keyframes scaleValue {
  0% {
    transform: scale(1)
  }
  70% {
    transform: scale(3) translateX(0)
  }
  100% {
    transform: scale(1) translateX(0)
  }
}

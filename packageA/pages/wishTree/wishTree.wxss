@keyframes upone {
  0% {
    transform: translate(0,0) scale(1);
    opacity: 0;
  }
  5% {
    transform: translate(0,-10rpx) scale(1);
    opacity: 1;
  }
  60% {
    transform: translate(0,-550rpx) scale(2.5);
    opacity: 1;
  }
  100% {
    transform: translate(0, -840rpx) scale(2);
    opacity: 0;
  }
}
@keyframes uponex {
  0% {
    transform: translate(0,0) scale(1);
    opacity: 0;
  }
  5% {
    transform: translate(0,-20rpx) scale(1);
    opacity: 1;
  }
  60% {
    transform: translate(0,-600rpx) scale(3);
    opacity: 1;
  }
  100% {
    transform: translate(0, -1000rpx) scale(2);
    opacity: 0;
  }
}

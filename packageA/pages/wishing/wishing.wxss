/* 愿望 */
.wish-item {
  position: absolute;
  width: 122rpx;
  text-align: center;
  animation: wishmovedown 2s infinite linear; 
}
/* 愿望动画 */
@keyframes wishmovedown{
  0% {
    transform: translateY(0);
  }
	50% {
		transform: translateY(10rpx);
	}
	100% {
		transform: translateY(0);
	}
}
@keyframes wishmoveup{
  0% {
    transform: translateY(0);
  }
	50% {
		transform: translateY(-10rpx);
	}
	100% {
		transform: translateY(0);
	}
}
@keyframes wishmove{
  0% {
    transform: translate(0,0);
  }
	25% {
		transform: translate(10rpx,10rpx);
	}
	50% {
		transform: translate(0,0);
	}
  75% {
		transform: translate(-10rpx,10rpx);
	}
  100% {
		transform: translate(0,0);
	}
}
.wish-item:nth-child(1) {
  left: 330rpx;
  top: 114rpx;
  animation: wishmove 3s infinite linear;
}
.wish-item:nth-child(2) {
  left: 214rpx;
  top: 22rpx;
  animation: wishmoveup 2s infinite linear; 
}
.wish-item:nth-child(3) {
  left: 80rpx;
  top: 134rpx;
}
.wish-item:nth-child(4) {
  right: 90rpx;
  top: 114rpx;
}
.wish-item:nth-child(5) {
  right: 202rpx;
  top: 306rpx;
}
.wish-item:nth-child(6) {
  left: 188rpx;
  top: 344rpx;
  animation: wishmoveup 2s infinite linear; 
}

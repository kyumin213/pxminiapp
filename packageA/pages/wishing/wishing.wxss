}
.light-item-x:nth-child(4) {
  top: 600rpx;
}
.light-item-x:nth-child(5) {
  top: 730rpx;
}
@keyframes rightcirclemove{
  0% {
    transform: translate(0,0);
  }
  10% {
    transform: translate(15rpx,12rpx);
  }
  20% {
    transform: translate(30rpx,24rpx);
  }
  30% {
    transform: translate(30rpx,36rpx);
  }
  40% {
    transform: translate(15rpx,48rpx);
  }
  50% {
    transform: translate(0,60rpx);
  }
  60% {
    transform: translate(-15rpx,48rpx);
  }
  70% {
    transform: translate(-30rpx,36rpx);
  }
  80% {
    transform: translate(-30rpx,24rpx);
  }
  90% {
    transform: translate(-15rpx,12rpx);
  }
  100% {
    transform: translate(0,0);
  }	
}
@keyframes rightcirclemove2{
  0% {
    transform: translate(0,0);
  }
	25% {
		transform: translate(40rpx,15rpx);
	}
	50% {
		transform: translate(0,40rpx);
	}
  75% {
		transform: translate(-40rpx,15rpx);
	}
  100% {
		transform: translate(0,0);
	}
}
@keyframes leftcirclemove2{
  0% {
    transform: translate(0,0);
  }
	10% {
		transform: translate(-10rpx,15rpx);
	}
  25% {
		transform: translate(-20rpx,25rpx);
	}
	40% {
		transform: translate(-40rpx,30rpx);
	}
  55% {
		transform: translate(0,20rpx);
	}
  70% {
		transform: translate(15rpx,25rpx);
	}
  85% {
		transform: translate(40rpx,15rpx);
	}
  100% {
		transform: translate(0,0);
	}
}
@keyframes leftcirclemove{
  0% {
    transform: translate(0,0);
  }
	25% {
		transform: translate(-30rpx,15rpx);
	}
	50% {
		transform: translate(0,30rpx);
	}
  75% {
		transform: translate(30rpx,15rpx);
	}
  100% {
		transform: translate(0,0);
	}
}
